-SyntaxOFFunctions{
# use from etu function object
printf=function(name='printf',args=["string:str","formated"],variables={"string=string.format(formated)"},lines=["print(string)"]).exe();
}

-SyntaxOFImport{
#omg files
import_omg("etu.omg");
