}

-SyntaxOFClasses{
# use from etu class object
Class("ClassName",functions={"for initialize the class/builtin-Function" : {"name":"__init__",args=(self,),lines=["pass"]},"print class name/pcn":{"name" : "pcn" , args=(self,),lines=["print(self.__name__)"]}}).give();
}

-SyntaxOFUseLib{
# we use etulib
etu = importpy("etu");
etu->easy_loading(armor='['+etu.Back.GREEN,armorb=etu.Back.RESET+']',loader=' ',range_=100,sleep=0.01);
}
